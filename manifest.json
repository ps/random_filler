{
  // Required
  "manifest_version": 2,
  "name": "Random Filler",
  "version": "1.0",
  "options_page": "options.html",

  // Recommended
  "description": "Web developer tool that fills forms with potential edge cases and random data!",
  "icons": {},

  "background":{
    "scripts" : ["back.js"]
  },

  "browser_action": {
    "default_icon" : {
      "19" : "icon19.png"
    },
    "default_title": "Random Filler"//,
//    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab"
  ]

}
