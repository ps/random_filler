{
  // Required
  "manifest_version": 2,
  "name": "Random Filler",
  "version": "1.0",
  "options_page": "options.html",

  // Recommended
  "description": "Simple tool that fills forms with random data or predefined edge cases.",
  "background":{
    "scripts" : ["back.js"]
  },
  "icons": {
      "256": "icons/icon256.png"
  },
  "browser_action": {
    "default_icon" : {
      "19" : "icons/icon24.png"
    },
    "default_title": "Random Filler"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}